{
  "name": "zfegg/doctrine-helper",
  "description": "Doctrine helpers.",
  "license": "MIT",
  "authors": [
    {
      "name": "Moln Xie",
      "email": "moln.xie@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "doctrine/orm": "^3.3",
    "psr/container": "^1.0 || ^2.0"
  },
  "autoload": {
    "psr-4": {
      "Zfegg\\DoctrineHelper\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ZfeggTest\\DoctrineHelper\\": "test/"
    }
  },
  "extra": {
    "laminas": {
      "config-provider": "Zfegg\\DoctrineHelper\\ConfigProvider"
    },
    "branch-alias": {
      "dev-master": "0.4.x-dev"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "slevomat/coding-standard": "^8.15",
    "symfony/serializer": "^7.0",
    "laminas/laminas-servicemanager": "^4.0",
    "roave/psr-container-doctrine": "^5.5",
    "symfony/console": "^6.0 || ^7.0"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
